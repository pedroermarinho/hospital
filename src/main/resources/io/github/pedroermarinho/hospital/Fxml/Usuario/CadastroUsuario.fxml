                </HBox>
            </bottom>
            <center>
                <VBox alignment="CENTER" focusTraversable="true" maxHeight="1.7976931348623157E308" maxWidth="320.0" minWidth="200.0" prefHeight="413.0" prefWidth="250.0" spacing="10.0" BorderPane.alignment="CENTER">
                    <children>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <TextField fx:id="PrimeiroNomeField" nodeOrientation="LEFT_TO_RIGHT" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nome" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <TextField fx:id="SobrenomeField" nodeOrientation="LEFT_TO_RIGHT" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sobrenome" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <TextField fx:id="usuarioField" alignment="BOTTOM_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Usuario" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <TextField fx:id="EmailField" alignment="BOTTOM_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <PasswordField fx:id="senhaField" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Senha" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
                            <items>
                                <DatePicker fx:id="dpData" maxHeight="32.0" minHeight="32.0" prefHeight="32.0" prefWidth="210.0" />
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Data" />
                            </items>
                        </ToolBar>
                        <ToolBar maxWidth="330.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="228.0">
