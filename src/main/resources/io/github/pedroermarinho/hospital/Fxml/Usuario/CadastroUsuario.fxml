<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" styleClass="background-black-1d" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.pedroermarinho.hospital.Controller.Usuario.CadastroUsuarioController">
    <children>
        <BorderPane prefHeight="500.0" prefWidth="394.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <bottom>
                <HBox alignment="CENTER" prefHeight="35.0" prefWidth="200.0" spacing="15.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="btnCancelar" maxHeight="35.0" minHeight="30.0" mnemonicParsing="false" onAction="#OnCancelar" prefHeight="35.0" prefWidth="150.0" styleClass="button-red" text="Cancelar" />
                        <Button fx:id="btnSalvar" maxHeight="35.0" minHeight="30.0" mnemonicParsing="false" onAction="#OnSalvar" prefHeight="35.0" prefWidth="150.0" styleClass="button-verde" text="Salvar" />
